{
  "@context": {
    "dc":           "http://purl.org/dc/terms/",
    "doap":         "http://usefulinc.com/ns/doap#",
    "earl":         "http://www.w3.org/ns/earl#",
    "foaf":         "http://xmlns.com/foaf/0.1/",
    "mf":           "http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#",
    "qt":           "http://www.w3.org/2001/sw/DataAccess/tests/test-query#",
    "rdfatest":     "http://rdfa.info/vocabs/rdfa-test#",
    "rdfs":         "http://www.w3.org/2000/01/rdf-schema#",
    "assertedBy":   {"@id": "earl:assertedBy", "@type": "@id"},
    "assertions":   {"@type": "@id", "@container": "@list"},
    "data":         {"@id": "qt:data", "@type": "@id"},
    "description":  {"@id": "dc:description"},
    "developer":    {"@id": "doap:developer", "@type": "@id"},
    "doap_desc":    {"@id": "doap:description"},
    "entries":      {"@id": "mf:entries", "@type": "@id", "@container": "@list"},
    "homepage":     {"@id": "doap:homepage", "@type": "@id"},
    "html4":        {"@id": "rdfatest:hostLanguage/html4", "@container": "@list"},
    "html5":        {"@id": "rdfatest:hostLanguage/html5", "@container": "@list"},
    "label":        {"@id": "rdfs:label"},
    "language":     {"@id": "doap:programming-language"},
    "mode":         {"@id": "earl:mode", "@type": "@id"},
    "name":         {"@id": "doap:name"},
    "num":          {"@id": "rdfatest:num"},
    "outcome":      {"@id": "earl:outcome", "@type": "@id"},
    "processor":    {"@id": "rdfatest:processor", "@collection": "@set"},
    "query":        {"@id": "qt:query", "@type": "@id"},
    "queryForm":    {"@id": "qt:queryForm", "@type": "@id"},
    "result":       {"@id": "earl:result"},
    "subject":      {"@id": "earl:subject", "@type": "@id"},
    "svg":          {"@id": "rdfatest:hostLanguage/svg", "@container": "@list"},
    "test":         {"@id": "earl:test", "@type": "@id"},
    "testAction":   {"@id": "mf:action", "@type": "@id"},
    "testResult":   {"@id": "mf:result", "@type": "xsd:boolean"},
    "title":        {"@id": ["dc:title", "mf:name"]}
